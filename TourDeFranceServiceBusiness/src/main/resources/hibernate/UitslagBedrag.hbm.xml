<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="nl.doyle.mccloud.tourdefrance.valueobjects.UitslagBedrag" table="UITSLAG_BEDRAG">
        <composite-id>
			<key-property name="categorie" column="CATEGORIE"/>
			<key-property name="positie" column="POSITIE"/>
		</composite-id>
        <!-- Type geeft aan waar de java.util.Date op moet mappen (SQL date, timestamp of time -->
        <property name="bedrag" column="BEDRAG"/>
    </class>
</hibernate-mapping>
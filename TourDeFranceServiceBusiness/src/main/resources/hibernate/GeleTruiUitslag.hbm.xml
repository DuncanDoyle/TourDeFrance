<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="nl.doyle.mccloud.tourdefrance.valueobjects.GeleTruiUitslag" table="GELETRUIUITSLAG">
        <composite-id>
			<key-property name="etappenummer" column="ETAPPENUMMER"/>
			<key-property name="positie" column="POSITIE"/>
		</composite-id>
		<many-to-one name="renner"
			column="RENNERNUMMER"
			not-null="true" lazy="false"/>
	</class>
</hibernate-mapping>
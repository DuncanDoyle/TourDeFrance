<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="nl.doyle.mccloud.tourdefrance.valueobjects.Renner" table="RENNER">
        <id name="nummer" column="NUMMER">
           <!-- Primary key generator gebruiken we niet omdat de primary key het rennernummer is 
           		<generator class="native"/>  
           -->
        </id>
		<property name="voornaam" column="VOORNAAM"/>
        <property name="achternaam" column="ACHTERNAAM"/>
    </class>
</hibernate-mapping>